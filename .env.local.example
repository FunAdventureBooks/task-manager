# Supabase credentials — get these from https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Simple password gate for the app (set any password you want)
NEXT_PUBLIC_APP_PASSWORD=changeme

# Supabase Service Role Key — used by OpenClaw for API access (NEVER expose in frontend)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
